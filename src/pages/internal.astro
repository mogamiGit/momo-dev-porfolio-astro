---
import Layout from "../layouts/Layout.astro";
import Header from "../layouts/Header.astro";
import Carousel from "../components/InternalProjectCards/Carousel.tsx";
import { projects } from "../components/InternalProjectCards/data.ts"
---

<Layout title="Internal Projects" description="Internal Projects Mogami">
  <Header />
  <main>
    <section class="w-full mx-auto flex items-center justify-center px-6 xl:px-0 mt-[150px]">
      <Carousel client:load projects={projects} />
    </section>
  </main>
</Layout>